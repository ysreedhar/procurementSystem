Public Sub flex_data()
On Error Resume Next
Dim fldata As New ADODB.Recordset
If fldata.State Then fldata.Close
fldata.Open "select * from releasestrategy", Cn, 3, 2

With flex_grid
    .Rows = 1
    While Not fldata.EOF
        .Rows = .Rows + 1
        .TextMatrix(.Rows - 1, 0) = fldata(0)
        .TextMatrix(.Rows - 1, 1) = fldata(1)
        .TextMatrix(.Rows - 1, 2) = fldata(2)
        .TextMatrix(.Rows - 1, 3) = fldata(3)
        .TextMatrix(.Rows - 1, 4) = fldata(4)
        .TextMatrix(.Rows - 1, 5) = fldata!flg
        .TextMatrix(.Rows - 1, 6) = fldata!amount
        .TextMatrix(.Rows - 1, 7) = fldata!amountmax
        .TextMatrix(.Rows - 1, 8) = fldata!expensetype
        fldata.MoveNext
    Wend
End With
End Sub






