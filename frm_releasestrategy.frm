Dim id As Double
id = 0
If flex_grid.TextMatrix(flex_grid.Row, 0) = "" Then Exit Sub
id = flex_grid.TextMatrix(flex_grid.Row, 0)

Dim sh As New ADODB.Recordset
If sh.State Then sh.Close
sh.Open "select * from releasestrategy where rs_id=" & id, Cn, 3, 2
If Not sh.EOF Then
releasestrategy.txt_code.Text = sh!rs_code
releasestrategy.txt_name.Text = sh!rs_name
releasestrategy.txt_desc.Text = sh!rs_desc
        If sh!prpo = releasestrategy.opt_msr.Caption Then
        releasestrategy.opt_msr.Value = True
        ElseIf sh!prpo = releasestrategy.opt_po.Caption Then
        releasestrategy.opt_po.Value = True
        End If
        If sh!flg = "Yes" Then
        releasestrategy.Check1.Value = 1
        Else
        releasestrategy.Check1.Value = 0
